{
  "name": "manuscripts",
  "version": "1.0.0",
  "description": "manuscripts app",
  "main": "index.js",
  "repository": "git@github.com:khirayama/manuscripts.git",
  "author": "khirayama",
  "license": "MIT",
  "private": "true",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "start": "npm run production",
    "production": "NODE_ENV=production npm run clean && npm run compile:scripts && npm run server",
    "server": "node dist/index.js",
    "clean": "rm -rf dist",
    "compile:scripts": "babel src --out-dir dist",
    "compile:scripts:watch": "babel src --out-dir dist -w --source-maps inline",
    "lint:scripts": "eslint --ignore-path .gitignore src",
    "lint:scripts:fix": "eslint --ignore-path .gitignore --fix src"
  },
  "dependencies": {
    "express": "^4.14.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-preset-es2015": "^6.22.0",
    "eslint": "^3.14.1",
    "eslint-config-xo-space": "^0.15.0"
  }
}
